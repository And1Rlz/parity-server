{
  "name": "parity-server",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Job processing background for parity league stats",
  "author": "Kevin Hughes <kevinhughes27@gmail.com>",
  "homepage": "http://github.com/kevinhughes27/parity-server",
  "repository": {
    "type": "git",
    "url": "http://github.com/kevinhughes27/parity-server"
  },
  "engines": {
    "node": "6.6.0",
    "npm": "3.10.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.16.0",
    "body-parser": "^1.13.3",
    "dotenv": "^1.2.0",
    "express": "^4.14.0",
    "lodash": "^4.16.1",
    "mongodb": "^2.2.10",
    "mongoskin": "^2.1.0",
    "monk": "^3.1.2",
    "morgan": "^1.7.0"
  },
  "devDependencies": {
    "glob": "^7.1.0",
    "loopnext": "0.0.3",
    "chai": "^3.2.0",
    "mocha": "^2.3.2",
    "mocha-sinon": "^1.1.4",
    "nodemon": "^1.10.2",
    "request": "^2.61.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  },
  "scripts": {
    "postinstall": "cd public && npm install",
    "heroku-postbuild": "npm run build",
    "build": "cd public && npm run build",
    "start": "babel-node server.js",
    "auto-start": "nodemon --exec 'npm start' --ignore public",
    "start:frontend": "cd public && npm start",
    "test:frontend": "cd public && npm test",
    "test": "mocha --compilers js:babel-core/register --recursive --reporter spec",
    "db:seed": "babel-node bin/seed.js",
    "db:reset": "echo 'db.dropDatabase()' | mongo development",
    "deploy": "git push heroku"
  }
}
